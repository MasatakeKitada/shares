<div class="container">
  <p class="search_result">検索結果</p>
  <div class="search_partial"></div>
  <% if !@pages.blank? %>
  <% @pages.each do |page| %>
	<div class="page col-xs-4" style="background-image: url(<%= page.image_url %>); background-size: cover;">
	  <div class="search_page_title">
	    <%= link_to "#{page.title}", "#{page.url}", target: "_blank" %>
	  </div>
	</div>
  <% end %>
  <% end %>
  </div>